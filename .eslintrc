{
  "extends": "airbnb",
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "rules": {
    "no-use-before-define": 0,
    "arrow-body-style": 0,
    "radix": 0,
    "new-cap": 0,
    "max-len": 0,
    "no-console": 0,
    "global-require": 0
  },
  "globals": {
    "__DEV__": true
  }
}
